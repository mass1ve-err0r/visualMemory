<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="apRoot"
            prefHeight="250.0"
            prefWidth="400.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="de.saadatbaig.visualMemory.Controllers.HomeViewController">
    <children>
        <Button fx:id="buttonCreateMemory" onAction="#createBlockView" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Create Memory View" textAlignment="CENTER" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="buttonSettings" onAction="#openSettings" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="Settings" textAlignment="CENTER" AnchorPane.bottomAnchor="75.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Label fx:id="labelBottomText" onMouseClicked="#openTheSauce" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="219.0" text="Made with ♥︎ in Germany" textAlignment="CENTER" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
    </children>
</AnchorPane>